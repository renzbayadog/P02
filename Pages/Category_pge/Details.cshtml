@page "/Categories/{id?}"
@using IdigitalCafe.Pages.Category_pge.Category_pge
@model DetailsModel

@{
    Layout = null;
}
 <div class="modal-header">
     <h4 class="modal-title" id="myModalLabel">Category Details</h4>
 </div>
 <div class="modal-body">
     <dl class="dl-horizontal">
       <dt>@Html.DisplayNameFor(model => model.Category.CategoryName):</dt>
						<dd>@Html.DisplayFor(model => model.Category.CategoryName)</dd>

     </dl>
 </div>
 <div class="modal-footer">
    <button type="submit" class="btn btn-primary btn-custom" data-dismiss="modal"><i class="fa fa-close"></i> Close</button>
 </div>